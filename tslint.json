{
  "extends": ["tslint:recommended", "tslint-config-airbnb"],
  "rules": {
    "import-name": false,
    "max-line-length": [true, 200],
    "indent": [true, "spaces", 2],
    "ter-arrow-parens": [true, "as-needed"],
    "object-shorthand-properties-first": false,
    "prefer-template": false,
    "no-console": true
  }
}